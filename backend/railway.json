{
  "build": {
    "env": {
      "OPENAI_API_KEY": {
        "required": true
      },
      "OPENAI_MODEL": {
        "required": false
      },
      "OPENAI_REALTIME_MODEL": {
        "required": false
      },
      "OPENAI_REALTIME_VOICE": {
        "required": false
      }
    },
    "commands": [
      "pip install -r requirements.txt"
    ]
  },
  "start": {
    "command": "uvicorn app.main:app --host 0.0.0.0 --port $PORT"
  }
}